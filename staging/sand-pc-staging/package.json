{
  "name": "sand-pc-staging",
  "version": "1.0.0",
  "description": "sand-pc脚手架",
  "author": "jianghe",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "init": "npm run clean && npm i && npm run start",
    "clean": "rm -rf ./node_modules ./dist ./package-lock.json ./coverage",
    "build": "sand-build build -t pc",
    "start": "sand-build start -t pc",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:style:fix": "npm run lint:style -- --fix",
    "test": "jest",
    "ci": "npm run lint:fix && npm run lint:style:fix && npm run test && npm run lint && npm run lint:style"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run ci",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "husky": "^4.2.5",
    "eslint": "~7.2.0",
    "stylelint": "^13.6.1",
    "jest": "^26.1.0",
    "@jianghe/sand-build": "^1.2.3-2",
    "@jianghe/sand-lint": "^1.2.3-2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "^4.4.0",
    "debug": "^4.1.1",
    "@jianghe/sand-core": "^1.2.3-2"
  }
}
